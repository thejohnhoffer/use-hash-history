{
  "name": "use-hash-history",
  "version": "0.1.2",
  "description": "A history hook for react-router-dom",
  "scripts": {
    "lint": "eslint src",
    "build": "rm -rf build && tsc -b",
    "format": "prettier --ignore-path .eslintignore --write .",
    "debug": "node --inspect-brk node_modules/uvu/bin.js -r tsm -r esm debug",
    "test": "uvu -r tsm -r esm tests",
    "demo": "vite serve demo --host",
    "publish": "pnpm build && npm publish"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "license": "MIT",
  "author": "John T. Hoffer",
  "repository": "github:thejohnhoffer/use-hash-history",
  "exports": {
    ".": "./build/index.js"
  },
  "files": [
    "./build/index.js",
    "./build/index.d.ts"
  ],
  "keywords": [
    "history",
    "location",
    "hook",
    "react"
  ],
  "peerDependencies": {
    "history": "5.x"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@testing-library/react": "^13.0.0-alpha.5",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-react": "^1.1.3",
    "eslint": "^8.4.1",
    "esm": "^3.2.25",
    "hast-util-to-html": "^8.0.3",
    "hastscript": "^7.0.2",
    "history": "^5.1.0",
    "linkedom": "^0.13.0",
    "prettier": "^2.5.1",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "react-router-dom": "^6.1.1",
    "tsm": "^2.2.1",
    "typescript": "^4.5.4",
    "uvu": "^0.5.2",
    "vite": "^2.7.2"
  }
}
